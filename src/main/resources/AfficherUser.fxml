                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="forumbutton" layoutX="989.0" layoutY="13.0" mnemonicParsing="false" onAction="#goToForum" style="-fx-background-color: #3F8EFC;" text="Forum" textFill="WHITE">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </Button>
           </children>
       </Pane>
      <Pane layoutX="30.0" layoutY="35.0" prefHeight="414.0" prefWidth="712.0">
         <children>
            <TableView fx:id="idtable" layoutX="44.0" layoutY="56.0" prefHeight="282.0" prefWidth="511.0">
              <columns>
                <TableColumn fx:id="userid" prefWidth="75.0" text="Id User" />
                <TableColumn fx:id="fullnameid" prefWidth="75.0" text="full Name" />
                  <TableColumn fx:id="emailid" prefWidth="96.800048828125" text="Email" />
                  <TableColumn fx:id="dateofbirthid" prefWidth="79.199951171875" text="date of birth" />
                  <TableColumn fx:id="passwordid" prefWidth="85.5999755859375" text="password" />
                  <TableColumn fx:id="roleid" prefWidth="85.5999755859375" text="role" />
              </columns>
            </TableView>
            <Button fx:id="addNew" layoutX="587.0" layoutY="56.0" mnemonicParsing="false" onAction="#goToAjouterUser" text="Add New" />
            <Button fx:id="updateid" layoutX="587.0" layoutY="136.0" mnemonicParsing="false" onAction="#UpdateUser" text="Update" />
            <Button fx:id="deleteid" layoutX="582.0" layoutY="172.0" mnemonicParsing="false" onAction="#deleteUser" prefWidth="65.0" text="Delete" />
         </children>
      </Pane>
   </children>
