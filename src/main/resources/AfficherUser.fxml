<AnchorPane prefHeight="516.0" prefWidth="815.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="esprit.tn.controllers.AfficherUser">
   <children>
       <Pane layoutX="-6.0" prefHeight="45.0" prefWidth="1072.0" style="-fx-background-color: #3F8EFC;">
           <children>
               <ImageView fitHeight="44.0" fitWidth="45.0" layoutX="14.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
                   <image>
                       <Image url="@logo.png" />
                   </image>
               </ImageView>
            <Button fx:id="coursbutton" layoutX="771.0" layoutY="11.0" mnemonicParsing="false" onAction="#gotocours" style="-fx-background-color: #3F8EFC;" text="Cours" textFill="WHITE">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="seancebutton" layoutX="818.0" layoutY="12.0" mnemonicParsing="false" style="-fx-background-color: #3F8EFC;" text="Seance" textFill="WHITE">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="reclamationbutton" layoutX="865.0" layoutY="12.0" mnemonicParsing="false" onAction="#afficherReclamation" style="-fx-background-color: #3F8EFC;" text="Reclamation" textFill="WHITE">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
