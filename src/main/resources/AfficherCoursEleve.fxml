<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="751.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="esprit.tn.controllers.AfficherCoursEleveController">
    <Pane layoutX="-6.0" prefHeight="45.0" prefWidth="1072.0" style="-fx-background-color: #3F8EFC;">
        <children>
            <ImageView fitHeight="44.0" fitWidth="45.0" layoutX="14.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@logo.png" />
                </image>
            </ImageView>
            <Button fx:id="coursbutton" layoutX="395.0" layoutY="11.0" mnemonicParsing="false" onAction="#gotocours" style="-fx-background-color: #3F8EFC;" text="Cours" textFill="WHITE">
                <font>
                    <Font name="Times New Roman Bold" size="12.0" />
                </font>
            </Button>
            <Button fx:id="absenceButton" layoutX="503.0" layoutY="12.0" mnemonicParsing="false" onAction="#goToAbsence" style="-fx-background-color: #3F8EFC;" text="Absence" textFill="WHITE">
                <font>
                    <Font name="Times New Roman Bold" size="12.0" />
                </font>
            </Button>
            <Button fx:id="reclamationbutton" layoutX="561.0" layoutY="12.0" mnemonicParsing="false" onAction="#reclamation" style="-fx-background-color: #3F8EFC;" text="Reclamation" textFill="WHITE">
                <font>
                    <Font name="Times New Roman Bold" size="12.0" />
                </font>
            </Button>
            <Button fx:id="quizbutton" layoutX="642.0" layoutY="13.0" mnemonicParsing="false" onAction="#goToQuiz" style="-fx-background-color: #3F8EFC;" text="Quiz" textFill="WHITE">
                <font>
                    <Font name="Times New Roman Bold" size="12.0" />
                </font>
            </Button>
            <Button fx:id="forumbutton" layoutX="685.0" layoutY="13.0" mnemonicParsing="false" onAction="#goToForum" style="-fx-background-color: #3F8EFC;" text="Forum" textFill="WHITE">
                <font>
                    <Font name="Times New Roman Bold" size="12.0" />
                </font>
            </Button>
         <Button fx:id="seancebutton" layoutX="449.0" layoutY="12.0" mnemonicParsing="false" onAction="#goToSeance" style="-fx-background-color: #3F8EFC;" text="Seance" textFill="WHITE">
            <font>
               <Font name="Times New Roman Bold" size="12.0" />
            </font>
         </Button>
        </children>
    </Pane>
    <Pane prefHeight="42.0" prefWidth="1072.0" style="-fx-background-color: #ADD7F6;">
        <children>


            <Label layoutX="325.0" layoutY="8.0" style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Courses List" />
        </children>

    </Pane>
    <TableView fx:id="courseTable" prefHeight="400.0" prefWidth="800.0">
        <columns>
            <TableColumn fx:id="titleColumn" prefWidth="134.8000060915947" text="Title" />
            <TableColumn fx:id="descriptionColumn" prefWidth="256.3999938964844" text="Description" />
            <TableColumn fx:id="statusColumn" prefWidth="104.20001220703125" text="Statut" />
            <TableColumn fx:id="levelColumn" prefWidth="131.4000244140625" text="Level" />
            <TableColumn fx:id="enrollColumn" prefWidth="161.4000244140625" text="Enroll">

            </TableColumn>
        </columns>
    </TableView>
</VBox>
